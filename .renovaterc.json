{
  "assignees": [
    "kevinmatthes"
  ],
  "commitMessageLowerCase": "never",
  "commitMessagePrefix": "[Renovate]",
  "dependencyDashboard": true,
  "dependencyDashboardAutoclose": true,
  "dependencyDashboardLabels": [
    "dependencies"
  ],
  "labels": [
    "dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": null
  },
  "packageRules": [
    {
      "addLabels": [
        "rust",
      ],
      "matchManagers": [
        "cargo"
      ]
    },
    {
      "addLabels": [
        "github_actions"
      ],
      "matchManagers": [
        "github-actions"
      ]
    },
    {
      "addLabels": [
        "documentation"
      ],
      "matchManagers": [
        "regex"
      ],
      "matchPackageNames": [
        "msrv"
      ]
    }
  ],
  "regexManagers": [
    {
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "msrv",
      "fileMatch": [
        "^\\.clippy\\.toml$"
      ],
      "matchStrings": [
        "^msrv = \\\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\\\"$"
      ],
      "packageNameTemplate": "rust-lang/rust"
    },
    {
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "msrv",
      "fileMatch": [
        "^Cargo\\.toml$"
      ],
      "matchStrings": [
        "^rust-version = \\\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\\\"$"
      ],
      "packageNameTemplate": "rust-lang/rust"
    },
    {
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "msrv",
      "fileMatch": [
        "^README\\.md$",
        "^src/lib\\.rs$"
      ],
      "matchStrings": [
        "MSRV-(?<currentValue>\\d+\\.\\d+\\.\\d+)-green"
      ],
      "packageNameTemplate": "rust-lang/rust"
    }
  ],
  "schedule": null
}
